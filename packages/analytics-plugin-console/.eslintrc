{
  "parser": "@babel/eslint-parser",
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true
  },
  "globals": {
  },
  "rules": {
    "array-bracket-spacing": [ "error", "always" ],
    "func-names": "off",
    "max-len": "off",
    "no-console": "off",
    "no-lonely-if": "off",
    "no-multiple-empty-lines": "off",
    "object-curly-newline": "off"
  }
}
